page{background-color:#f5f5f5;}


.successfully-main{}

.successfully-main .pic{
    width: 186rpx;
    height: 186rpx;
    margin: 100rpx auto 0;
}
.successfully-main .pic image{
    width: 186rpx;
    height: 186rpx;
}


.successfully-main .succ-text{
    font-weight:500;
    color:rgba(0,0,0,1);
    font-size: 40rpx;
    margin: 48rpx auto 0;
    text-align: center;
}
.successfully-main .succ-tips{
    font-weight:400;
    color:rgba(153,153,153,1);
    font-size: 28rpx;
    text-align: center;
    margin: 10rpx auto 0;
}

.successfully-main .write-off-record{
    font-weight:400;
    color:rgba(255,102,0,1);
    font-size: 28rpx;
    text-align: center;
    margin-top: 20rpx;
}
.successfully-main .write-off-record text{
    padding-left: 10rpx;
}


.successfully-main .ppload-btn{
  height:94rpx;
  background:rgba(40,199,133,1);
  border-radius:10rpx;
  border:2rpx solid rgba(5,5,5,0.08);
  text-align: center;
  line-height: 92rpx;
  box-sizing: border-box;
  margin: 40rpx 40rpx 0 40rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  font-size: 36rpx;
}
.successfully-main .close-btn{
  height:94rpx;
  background:rgba(248,248,248,1);
  border-radius:10rpx;
  border:2rpx solid rgba(5,5,5,0.1);
  line-height: 94rpx;
  text-align: center;
  margin: 34rpx 40rpx 0 40rpx;
  font-weight:400;
  color:rgba(0,0,0,1);
  font-size: 36rpx;
}


