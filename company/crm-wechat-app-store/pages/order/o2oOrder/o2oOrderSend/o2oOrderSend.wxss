  font-family: PingFangSC-Regular;
  font-size: 26rpx;
}

.model-warp {
  height: 100rpx;
}

.model-single {
  height: 100rpx;
  width: 92vw;
  justify-content: space-between;
  align-items: center;
  padding-left: 32rpx;
  padding-right: 32rpx;
  /* background-color: #118CFE; */
  background-color: #fff;
  position: absolute;
}

.swiperView {
  width: 100%;
  height: 100rpx;
  display: flex;
  overflow: hidden;
  flex-direction: row;
}

.del-item {
  height: 100rpx;
  width: 20%;
  background-color: #F43530;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 0rpx;
