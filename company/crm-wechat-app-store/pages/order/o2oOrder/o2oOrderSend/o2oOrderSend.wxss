  align-items: center;
}

.title-receipt {
  margin-top: 23rpx;
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
  line-height: 48rpx;
  color: #1a1a1a;
  letter-spacing: 0;
}

.infos-warp {
  margin-left: 20rpx;
