}

.del-item text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FFFFFF;
  text-align: center;
}

.model-line {
  margin-left: 32rpx;
}

.numPlusMinus {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 200rpx;
}

.numPlusMinus image {
  width: 56rpx;
  height: 56rpx;
}
.numPlusMinus text{
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #1a1a1a;
}

.model-scroll {
  width: 100%;
  height: 202rpx;
}

.addLogNums {
  width: 100%;
  height: 136rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.addLogNums text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #27C886;
}

.content {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding-left: 32rpx;
  padding-right: 64rpx;
  margin-right:0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px
}

.content input {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
}

.content .input-warp {
  width: 194rpx;
  height: 56rpx;
  border-radius: 28rpx;
  background-color: #F5F5F5;
  padding-left: 28rpx;
  padding-right: 28rpx;
}

.del {
  background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content{
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

