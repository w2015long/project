.elastic_layer{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.30);
    z-index:100;
}

.elastic_layer .layer{
  height: 500rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  z-index:100
}

.elastic_layer .layer .l_title{
  height: 93rpx;
  line-height: 93rpx;
  font-size: 30rpx;
  color: #1A1A1A;
  padding-left: 32rpx;
}


.elastic_layer .layer .delivery_method{
  height: 277rpx;
}
.elastic_layer .layer .delivery_method .method_items{
  display: inline-block;
  border: 1rpx solid #1A1A1A;
  border-radius: 29rpx;
  width: 178rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  font-size: 28rpx;
  margin-left: 32rpx;
  margin-top: 20rpx;
}
.elastic_layer .layer .delivery_method .cur{
  background: rgba(39,200,134,0.20);
  border: 1rpx solid #27C886;
  border-radius: 29rpx;
  color: #27C886;
}

.elastic_layer .layer .confirm_btn{
  background: #27C886;
  border-radius: 50rpx;
  width: 375rpx;
  height: 72rpx;
  text-align: center;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  margin: 0 auto;
}


