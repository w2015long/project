}
.item-value-icon image{
 width: 16rpx;
 height: 26rpx;
 display: block;
}


.title {
  line-height: 42rpx;
}

.bottom-btn {
  height: 100rpx;
  width: 100%;
  display: flex;
  background-color: #27C886;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0rpx;
}

.table {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.tr {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.th {
  display: flex;
  flex: 1;
  justify-content: center;
  margin-top: 18rpx;
}

.bg-w text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #9A9A9A;
  letter-spacing: 0;
}

.bg-b text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #1a1a1a;
  letter-spacing: 0;
}

.bg-b text {
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
}

.payAmount {
  margin-top: 18rpx;
  margin-bottom: 30rpx;
}

.payAmount text {
  font-size: 30rpx;
}

.bottom-btn text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #FFFFFF;
}
/*  订单发货UI  */
.sender-scroll {
  width: 100%;
  height: 88vh;
  overflow: hidden;
}

.operate-tab view {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
}

.operate-tab text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #1A1A1A;
  text-align: center;
}

.operate-tab {
   display: flex;
   flex-direction: row;
   height: 88rpx;
   width: 100%;
   background-color: #fff;
   margin-bottom: 20rpx;
}

.cell-line {
  height:1rpx;
  width:100vw;
  background-color: #E5E5E5
}

.addr-box {
  display: flex;
  padding-bottom: 13rpx;
  padding-top: 13rpx;
  padding-left: 32rpx;
  position: relative;
  overflow: hidden;
}
.border-bottom{position: relative;}
.border-bottom::before {
  content: " ";
  position: absolute;
  left: 32rpx;
  bottom: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
  z-index: 2;
}
.addr-box .addr-hd{
    font-size:28rpx;
    line-height: 40rpx;
    white-space: nowrap;
    color: #9A9A9A;
}
.addr-box .addr-bd{line-height: 40rpx;font-size:28rpx;}


