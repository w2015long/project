  margin-top: 17rpx;
}

.single-line {
  display: flex;
  flex-direction: row;
  margin-top: 5rpx;
}

.multiple {
  width: 87%;
}

.title-item {
  margin-left: -12rpx;
}

.item-title {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  line-height: 40rpx;
  padding-left:32rpx;
  color: #9A9A9A;
  white-space: nowrap;
}

.item-value {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #1a1a1a;
  line-height: 40rpx;
  letter-spacing: 0;
}
.item-value_p{
  position: absolute;
  right: 68rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 28rpx;
  color: #1A1A1A;
}
.item-value-icon{
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
