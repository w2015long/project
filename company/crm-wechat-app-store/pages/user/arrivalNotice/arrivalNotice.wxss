page{background-color: #f5f5f5;}
.arrival-notice .item{display: flex; justify-content:space-between; font-size: 30rpx; color: #000000; padding: 0 32rpx; height: 88rpx; line-height: 88rpx; background-color: #fff; position: relative;}
.arrival-notice .item::before{
  border-top: 1px solid #e5e5e5;color: #e5e5e5; width:100%; content: " ";height: 1;left:32rpx; right: 0; position: absolute;
  bottom: 0;-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY  (0.5);transform: scaleY(0.5);z-index: 2;}

.margin-top-20{margin-top:20rpx;}
.ft-box{padding: 340rpx 32rpx 0 32rpx;}
button.login-btn{background-color: #28C785; font-size: 34rpx;}
.button-hover.login-btn{background-color: #26b97c;}
button.login-btn[disabled][type=primary] {background-color:#7eddb6;}
